/dts-v1/;
#include "berlin2q.dtsi"

/ {
	model = "Marvell BG2-Q DMP";
	compatible = "marvell,berlin2q-dmp", "marvell,berlin2q", "marvell,berlin";

	memory {
		device_type = "memory";
		reg = <0x00000000 0x80000000>;
	};

	choosen {
		bootargs = "console=ttyS0,115200 earlyprintk";
	};
};

&sdhci1 {
	broken-cd;
	sdhci,wp-inverted;
	status = "okay";
};

&sdhci2 {
	broken-cd;
	bus-width = <8>;
	non-removable;
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c2 {
	status = "okay";
};

&uart0 {
	status = "okay";
};

&eth0 {
	status = "okay";
};
